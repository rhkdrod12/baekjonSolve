import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		
		
	}
}


class quest{
	
	int testCase;
	int dotNum;
	int dot[][];
		
	public void solve() {
		
		Scanner sc = new Scanner(System.in);
		
		testCase = sc.nextInt();
		
		for(int test = 0 ; test < testCase ; test++) {
			
			dotNum = sc.nextInt();
			
			dot = new int[dotNum][2];		
			
			for(int i = 0; i < dotNum ; i++) {
				for(int j = 0 ; j < 2 ; j++) {
					dot[i][j] = sc.nextInt();
				}				
			}
			
			
		}
		
		sc.close();
	}
	
	
	
}